class marker
{
	static void writing()
	{
		System.out.println("writing is good habit........");
	}
	static void writing(String name)
	{
		System.out.println("the name of marker is= "+name);
	}
	static void writing(float ink)
	{
		System.out.println("the marker ink capacity is= "+ink+"ml");
	}
}
class  Parker
{
	public static void main(String[] args) 
	{
		System.out.println("start of java code");
		marker.writing();
		marker.writing("Parker");
		marker.writing(1.5f);
		System.out.println("end of java code");
	}
}
