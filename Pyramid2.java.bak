class Pyramid2 
{
	public static void main(String[] args) 
	{ 
		int n=5;
		for(int row=n-1;row<=n;row--)
		{
			for(int star=1;(2*row)>star;star++)
			{
				System.out.print("* ");
			}
			for(int space=1;space<=n-row;space++)
			{
				System.out.print("  ");
			}
			
			System.out.println();
		}
		
     }
}
