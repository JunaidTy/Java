class Starpattern1 
{
	public static void main(String[] args) 
	{
		for(int row=1;row<=15;row++)
		{
			for(int col=1;col<=15;col++)
			{
				if(row==1  || row==15 || col==1 || col==15 )
				{
		          System.out.print("* ");
			     }
			    else
			    {
				     System.out.print("  ");
			     }
		     }
			  System.out.println();
	       }
      }
}
